{% block flashMsg %}
{% for message in app.flashes('success') %}
    <div id="success" class="alert alert-success">
        {{ message }}
    </div>
{% endfor %}
    {% for message in app.flashes('failure') %}
        <div class="alert alert-success">
            {{ message }}
        </div>
    {% endfor %}
{% endblock %}

